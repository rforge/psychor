import("stats", "graphics", "rgl", "scatterplot3d", "ape")
export(homals)
export(princals)
export(plot3dstatic)

S3method(print, homals)
S3method(summary, homals)
S3method(plot, homals)
S3method(plot3d, homals)
S3method(predict, homals)
S3method(print, predict.homals)
S3method(summary, predict.homals)
S3method(plot3dstatic, homals)

S3method(print, princals)
S3method(print, summary.princals)
S3method(summary, princals)
S3method(plot, princals)



